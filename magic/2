OBJ=./lib/can.o ./lib/paint.o ./lib/pthread.o ./lib/misc.o ./lib/cursor.o ./lib/main.o
TARGET=main
CC=gcc
$(TARGET):$(OBJ)
	$(CC) $(OBJ) -o $(TARGET) -lpthread
./lib/main.o:./src/main.c
	$(CC) -c $^
./lib/misc.o:./src/misc.c
	$(CC) -c $^
./lib/cursor.o:./src/cursor.c
	$(CC) -c $^
./lib/pthread.o:./src/pthread.c
	$(CC) -c $^
./lib/paint.o:./src/paint.c
	$(CC) -c $^
./lib/can.o:./src/can.c
	$(CC) -c $^
.PHONLY:clean
clean:
	rm -rf $(OBJ)
