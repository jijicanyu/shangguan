1 计算100天以后的日期
2 timelog

time_t stamp;
time(&stamp);
stamp = time(NULL);
struct tm *tm;
tm = localtime(&stamp);
char buf[1024];
strftime(buf,1024,"%Y-%m-%d",tm);
puts(buf);





